{
    "swagger": "2.0",
    "info": {
        "title": "Laravel and Swagger",
        "description": "Getting started with Laravel and Swagger",
        "termsOfService": "",
        "contact": {
            "email": "name@example.com"
        },
        "version": "1.0.0"
    },
    "host": "donate/",
    "basePath": "/",
    "schemes": [
        "http"
    ],
    "paths": {
        "/api/posts": {
            "get": {
                "tags": [
                    "Posts"
                ],
                "summary": [
                    "Posts list"
                ],
                "description": "Return posts list",
                "parameters": [
                    {
                        "name": "order",
                        "in": "query",
                        "description": "Order Coulumn need to Order By, default is created_date",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "length",
                        "in": "query",
                        "description": "Length of record need to response",
                        "required": false,
                        "type": "int"
                    },
                    {
                        "name": "start",
                        "in": "query",
                        "description": "Start possition",
                        "required": false,
                        "type": "int"
                    },
                    {
                        "name": "Seach",
                        "in": "query",
                        "description": "Seach value",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "category",
                        "in": "query",
                        "description": "Category Id",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "orderDesc",
                        "in": "query",
                        "description": "orderDesc",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "properties": {
                                "data": {
                                    "type": "object"
                                },
                                "recordsTotal": {
                                    "type": "number"
                                },
                                "recordsFiltered": {
                                    "type": "number"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "422": {
                        "description": "Missing Data"
                    }
                }
            }
        },
        "/api/posts/{id}": {
            "get": {
                "tags": [
                    "Posts"
                ],
                "summary": [
                    "Post detail"
                ],
                "description": "Return post detail by Id",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Length of record need to response",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "properties": {
                                "id": {
                                    "type": "number",
                                    "example": 1
                                },
                                "user_id": {
                                    "type": "number",
                                    "example": 1
                                },
                                "title": {
                                    "type": "string",
                                    "example": "First Project"
                                },
                                "content": {
                                    "type": "string",
                                    "example": "Hello World"
                                },
                                "photo_thumbnail": {
                                    "type": "string",
                                    "example": "..."
                                },
                                "full_photo": {
                                    "type": "string",
                                    "example": "..."
                                },
                                "due_day": {
                                    "type": "string",
                                    "example": "2000-12-03 12:20:20"
                                },
                                "status": {
                                    "type": "number",
                                    "example": 0
                                },
                                "user": {
                                    "properties": {
                                        "id": {
                                            "type": "number",
                                            "example": 1
                                        },
                                        "first_name": {
                                            "type": "string",
                                            "example": "admin"
                                        },
                                        "last_name": {
                                            "type": "string",
                                            "example": "admin"
                                        },
                                        "username": {
                                            "type": "string",
                                            "example": "admin"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "admin@gmail.com"
                                        },
                                        "address": {
                                            "type": "string",
                                            "example": "Danang, Vietnam"
                                        },
                                        "code_id": {
                                            "type": "string",
                                            "example": "admincode"
                                        },
                                        "personal_photo": {
                                            "type": "string",
                                            "example": "..."
                                        },
                                        "gender": {
                                            "type": "number",
                                            "example": 0
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2000-12-03 12:20:20"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "example": "2000-12-03 12:20:20"
                                        }
                                    },
                                    "type": "object"
                                },
                                "created_at": {
                                    "type": "string"
                                },
                                "updated_at": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "422": {
                        "description": "Missing Data"
                    },
                    "404": {
                        "description": "NotFound Data"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    }
                }
            }
        }
    },
    "definitions": {}
}